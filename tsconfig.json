{
	"compilerOptions": {
		"experimentalDecorators": true,
		"lib": ["es6", "dom"],
		"module": "commonjs",
		"target": "es6",
		// "noImplicitAny": true,
		"suppressImplicitAnyIndexErrors": true,
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"sourceMap": false,
		"outDir": "../dist",
		"removeComments": false,
		"skipLibCheck": true
	}
}
